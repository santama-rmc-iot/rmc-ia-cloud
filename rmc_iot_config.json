	"CPU_info": {
		"objectKey": "com.atbridge-cnsltg.raspberrypi-1.shimura.CPUInfo",
		"objectType": "iaCloudObject",
		"objectDescription": "RaspberryPi CPU情報",
		"timeStamp": null,
		"options":{
			"period": 300,
			"source": "CPU_info"
		},
		"ObjectContent": {
			"contentType": "iaCloudData",
			"contentData": [{
				"dataName": "CPU温度",
				"dataValue": null,
				"unit": "°C",
				"options": {
					"source":"cat /sys/class/thermal/thermal_zone0/temp",
					"gain": 0.001,
					"offset": 0
				}
			},{
			"dataName": "CPU使用率",
			"dataValue": null,
			"unit": "%",
			"options": {
				"source":"vmstat | tail -1 | awk '{print $15}'",
				"gain": -1,
				"offset": 100
				}
			},{
			"dataName": "空きメモリ量",
			"dataValue": null,
			"unit": "MB",
			"options": {
				"source":"vmstat | tail -1 | awk '{print $4}'",
				"gain": 0.001,
				"offset": 0
				}
			}]
		}
	},
	"SENSORS_INFO": {
		"objectKey": "rmc-iot-santama.shimura.sensors",
		"objectType": "iaCloudObject",
		"objectDescription":  "センサーの値",
		"timestamp": null,
		"options":{
			"period": 60,
			"source": "SENSORS_INFO"
		},
		"ObjectContent": {
			"contentType": "iaCloudData",
			"contentData":[{
				"commonName": "Column1",
				"dataName": "CTセンサー",
				"dataValue": null,
				"unit": "V",
				"options":"ct_sensor_value",
				"gain": 16,
				"offset": 0
			},{
				"commonName": "Column2",
				"dataName": "超音波センサー",
				"dataValue": null,
				"unit": "cm",
				"options": "usonic_sensor_value"
			}]
		}
	}
